{
  "expo": {
    "name": "Safra Challenge",
    "slug": "safra-challenge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/index.png",
    "scheme": "safrachallenge",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/index.png",
        "backgroundColor": "#FFEB3B"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mohavvvvd.safrachallenge"
    },
    "splash": {
      "image": "./assets/images/index.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFEB3B"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/index.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/index.png",
          "resizeMode": "contain",
          "backgroundColor": "#FFEB3B"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0df1b066-1430-47fd-ba8f-a5b55e1fa5f8"
      }
    }
  }
}
